<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="diseaseName">

	<select id="getDisName" parameterType="int" resultType="string">
		select
		dis_nm from diseaseName where dis_id = #{dis_id}
	</select>

</mapper>















